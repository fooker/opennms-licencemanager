<?xml version="1.0" encoding="UTF-8"?>
## Note the velocity directives have to start after the xml definition line because they
## create a new line before the xml definition which effects XML parsing
#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<licenceMetadata>
  <productId>${groupId}/${rootArtifactId}/${version}</productId>
  <licensee></licensee>
  <licensor>OpenNMS UK</licensor>
  <systemId></systemId>
  <startDate></startDate>
  <expiryDate></expiryDate>
  <options>
    <option>
      <description>this is the description of option 1</description>
      <name>option1</name>
      <value></value>
    </option>
  </options>
</licenceMetadata>
